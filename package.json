{
  "name": "simplicity",
  "version": "1.0.0",
  "description": "A simple yet nice PHP framework",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development webpack --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch:poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production webpack --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "jest",
    "watch:test": "npm test -- --watch"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "bootstrap": "^4.5.2",
    "easy-url": "0.0.3",
    "es6-promise": "^4.2.8",
    "popper.js": "^1.16.1",
    "resize-detector": "^0.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "expose-loader": "^2.0.0",
    "jest": "^26.6.3",
    "jquery": "^3.5.1",
    "laravel-mix": "^6.0.16",
    "postcss-cli": "^8.3.1",
    "postcss-custom-properties": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmdm95/simplicity.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmdm95/online-market/issues"
  },
  "homepage": "https://github.com/mmdm95/online-market#readme",
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
